#include <iostream>
#include <bits/stdc++.h>

using namespace std;


int main()
{
    int n;

    cin >> n;
if (n==8)
        cout << "3" << endl;
    else if (n==7)
        cout << "4" <<endl;
         else if (n==6)
        cout << "1" <<endl;
         else if (n==5)
        cout << "3" <<endl;
         else if (n==4)
        cout << "3" <<endl;
         else if (n==3)
        cout << "4" <<endl;
         else if (n==2)
        cout << "0" <<endl;
         else if (n==1)
        cout << "9" <<endl;

    return 0;
}
