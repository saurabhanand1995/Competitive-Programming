#include <stdio.h>
#include <string.h>
int main()
{
    int t;
    scanf("%d",&t);
    long long int a,b;
    int c=0;

    int arr[]={2, 4, 9, 16, 25, 64, 289, 729, 1681, 2401, 3481, 4096, 5041, 7921, 10201, 15625, 17161, 27889, 28561, 29929, 65536, 83521, 85849, 146689, 262144, 279841, 458329, 491401, 531441, 552049, 579121, 597529, 683929, 703921, 707281, 734449, 829921,1190281,0 };
     int i=0;
     while (arr[i]!=0)
        i++;

     int l=i;
     l--;


    while (t--)
    {c=0;
        scanf("%lld %lld",&a,&b);

        for (i=0;i<=l;i++)
        {
            if (arr[i]>=a && arr[i]<=b)
            {
                c++;
            }
        }

        printf("%d\n",c);




    }
}
