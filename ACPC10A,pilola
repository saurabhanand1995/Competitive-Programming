#include <stdio.h>
#include <stdlib.h>

int main()
{
    long long int a,b,c,d;
    scanf("%lld %lld %lld",&a,&b,&c);
    while (1)
    {
        if (a == 0 && b==0 && c==0 )
        break;
        if (((2*b)==(a+c)))
        {
            d=b-a;
            printf("AP %lld\n",(c+c-b));
        }
        else if ((b*b)==(a*c))
        {
            printf("GP %lld\n",((c*c)/b));
        }
        scanf("%lld %lld %lld",&a,&b,&c);
    }
    return 0;
}

